{
  "queries": [
    {
      "project": ["A_name"],
      "pattern": ["Artist"]
    },
    {
      "project": ["R_ID"],
      "pattern": ["Record"],
      "filter": "R_name = 'value'"
    },
    {
      "project": ["T_ID"],
      "pattern": ["Track"],
      "filter": "T_title = 'value'"
    },
    {
      "project": ["A_ID", "R_ID", "T_ID"],
      "pattern": ["Has", "Made"]
    },
    {
      "project": ["R_ID"],
      "pattern": ["Artist", "Record", "Track", "Has", "Made"],
      "filter": "A_name = 'value1' AND R_name = 'value2'"
    },
    {
      "project": ["T_ID", "A_ID"],
      "pattern": ["Artist", "Record", "Track", "Has", "Made"],
      "filter": "T_title = 'value1' OR T_title = 'value2'"
    }
  ]
}