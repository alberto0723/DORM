{
  "queries": [
    {
      "project": ["A_ID"],
      "pattern": ["Author"]
    },
    {
      "project": ["B_title"],
      "pattern": ["Book"]
    },
    {
      "project": ["A_ID", "A_name"],
      "pattern": ["Author"]
    },
    {
      "project": ["Writes_Book", "Writes_Author"],
      "pattern": ["Writes"]
    },
    {
      "project": ["A_ID", "B_ID"],
      "pattern": ["Book", "Writes", "Author"]
    },
    {
      "project": ["Writes_Book"],
      "pattern": ["Author", "Writes"],
      "filter": "A_name = 'hello'"
    },
    {
      "project": ["A_ID", "B_ID"],
      "pattern": ["Author", "Book", "Writes"],
      "filter": "A_name = 'hello'"
    },
    {
      "project": ["*"],
      "pattern": ["Book"]
    },
    {
      "project": ["*"],
      "pattern": ["Author"]
    },
    {
